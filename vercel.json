{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/webinars/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/bootcamp/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/academy/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "github": {
    "silent": false,
    "autoAlias": true
  },
  "public": true,
  "alias": ["aide-brasil.ai", "www.aide-brasil.ai"]
}