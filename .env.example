# Environment Variables Template
# Copy this file to .env.local for local development

# API Configuration
VITE_API_URL=https://api.example.com
VITE_API_KEY=your_api_key_here

# Analytics (optional)
VITE_GA_TRACKING_ID=UA-XXXXXXXXX-X
VITE_HOTJAR_ID=XXXXXXX

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=false

# Social Links
VITE_DISCORD_INVITE=https://discord.gg/aidebrasil
VITE_GITHUB_REPO=https://github.com/owshq-manus-ai/aide-brasil-website

# Deployment
VITE_DEPLOY_URL=https://aide-brasil.ai
VITE_VERCEL_URL=https://aide-brasil-website.vercel.app

# n8n Webhook Configuration
# Each CTA can have its own webhook for better tracking and automation

# Premium waitlist webhook (Nível Premium button)
VITE_N8N_WEBHOOK_PREMIUM=https://primary-production-1ebc.up.railway.app/webhook/473f499e-bcf5-4ff8-b17d-08f4fee8b9a7

# Free tier signup webhook (Nível Gratuito button)
VITE_N8N_WEBHOOK_FREE=https://primary-production-1ebc.up.railway.app/webhook/your-free-webhook-id

# Newsletter subscription webhook (if you add a newsletter form)
VITE_N8N_WEBHOOK_NEWSLETTER=https://primary-production-1ebc.up.railway.app/webhook/your-newsletter-webhook-id

# Legacy support - falls back to this if specific webhooks not defined
VITE_N8N_WEBHOOK_URL=https://primary-production-1ebc.up.railway.app/webhook/473f499e-bcf5-4ff8-b17d-08f4fee8b9a7

# Control webhook behavior
VITE_N8N_ENABLED=true
VITE_TRACK_EVENTS=true
VITE_TRACK_UTM=true
VITE_TRACK_DEVICE=true
VITE_LOG_ERRORS=true
VITE_SHOW_ERRORS=false

# Note: All VITE_ prefixed variables are exposed to the client
# Never put secrets in VITE_ variables!